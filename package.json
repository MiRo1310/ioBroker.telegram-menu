{
  "name": "iobroker.telegram-menu",
  "version": "1.7.0",
  "description": "Easily create Telegram Menus",
  "author": {
    "name": "MiRo1310",
    "email": "michael.roling@gmx.de"
  },
  "homepage": "https://github.com/MiRo1310/ioBroker.telegram-menu",
  "license": "MIT",
  "keywords": [
    "iobroker",
    "telegram",
    "menu"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/MiRo1310/ioBroker.telegram-menu.git"
  },
  "engines": {
    "node": ">= 18"
  },
  "dependencies": {
    "@iobroker/adapter-core": "^3.0.4",
    "@types/module-alias": "^2.0.4",
    "module-alias": "^2.2.3"
  },
  "devDependencies": {
    "@alcalzone/release-script": "^3.6.0",
    "@alcalzone/release-script-plugin-iobroker": "^3.6.0",
    "@alcalzone/release-script-plugin-license": "^3.5.9",
    "@alcalzone/release-script-plugin-manual-review": "^3.5.9",
    "@emotion/react": "^11.11.1",
    "@emotion/styled": "^11.11.0",
    "@iobroker/adapter-dev": "^1.2.0",
    "@iobroker/adapter-react-v5": "^4.9.3",
    "@iobroker/testing": "^4.1.0",
    "@mui/icons-material": "^5.15.2",
    "@mui/lab": "^5.0.0-alpha.158",
    "@mui/material": "^5.14.18",
    "@mui/styled-engine-sc": "^6.0.0-alpha.10",
    "@mui/styles": "^5.14.16",
    "@tsconfig/node20": "^20.1.4",
    "@types/chai": "^4.3.11",
    "@types/chai-as-promised": "^7.1.8",
    "@types/glob": "^8.1.0",
    "@types/mocha": "^10.0.3",
    "@types/node": "^20.10.1",
    "@types/proxyquire": "^1.3.31",
    "@types/sinon": "^17.0.2",
    "@types/sinon-chai": "^3.2.10",
    "@typescript-eslint/eslint-plugin": "^7.12.0",
    "chai": "^4.3.10",
    "chai-as-promised": "^7.1.1",
    "eslint": "^8.52.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-prettier": "^5.1.2",
    "eslint-plugin-react": "^7.34.2",
    "fs": "0.0.1-security",
    "inquirer": "^9.2.11",
    "json5": "^2.2.3",
    "mocha": "^10.2.0",
    "prettier": "^3.0.3",
    "proxyquire": "^2.1.3",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "sinon": "^17.0.1",
    "sinon-chai": "^3.7.0",
    "styled-components": "^6.1.0",
    "typescript": "~5.2.2"
  },
  "main": "build/src/main.js",
  "files": [
    "admin{,/!(src)/**}/!(tsconfig|tsconfig.*|.eslintrc).json",
    "admin{,/!(src)/**}/*.{html,css,png,svg,jpg,js}",
    "admin/build",
    "lib/",
    "www/",
    "io-package.json",
    "LICENSE",
    "build/src/",
    "admin/**/style.css",
    "admin/build/index.js.map"
  ],
  "_moduleAliases": {
    "@backend": "build/src/"
  },
  "scripts": {
    "prebuild": "rimraf admin/build",
    "build": "build-adapter react",
    "watch": "build-adapter react --watch",
    "prebuild:react": "rimraf admin/build",
    "build:react": "build-adapter react",
    "watch:react": "build-adapter react --watch",
    "test:js": "mocha --config test/mocharc.custom.json \"{!(node_modules|test)/**/*.test.js,*.test.js,test/**/test!(PackageFiles|Startup).js}\"",
    "test:package": "mocha test/package --exit",
    "test:integration": "mocha test/integration --exit",
    "test": "npm run test:js && npm run test:package",
    "check": "tsc --noEmit -p tsconfig.check.json",
    "lint": "eslint --ext .ts,.tsx src/ admin/src/",
    "translate": "translate-adapter",
    "release": "release-script",
    "lint:fix": "eslint . --fix --ext .ts,.tsx,.js,.jsx",
    "prettier": "prettier --write \"src/**/*.ts\" \"src/**/*.tsx\" \"admin/src/**/*.ts\" \"admin/src/**/*.tsx\"",
    "checker": "node skript.mjs",
    "dev": " dev-server run",
    "reset": "git reset $(git merge-base main $(git rev-parse --abbrev-ref HEAD))"
  },
  "bugs": {
    "url": "https://github.com/MiRo1310/ioBroker.telegram-menu/issues"
  },
  "readmeFilename": "README.md"
}
